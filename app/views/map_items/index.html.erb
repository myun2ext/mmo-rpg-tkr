<h1>Listing map_items</h1>

<table>
  <thead>
    <tr>
      <th>Map chip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @map_items.each do |map_item| %>
      <tr>
        <td><%= map_item.map_chip %></td>
        <td><%= link_to 'Show', map_item %></td>
        <td><%= link_to 'Edit', edit_map_item_path(map_item) %></td>
        <td><%= link_to 'Destroy', map_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Map item', new_map_item_path %>
